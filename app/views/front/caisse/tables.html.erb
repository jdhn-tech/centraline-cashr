
<div class="content">
<input id="itest" list="test">
<datalist id="test">
	<option value="One">
	<option value="Two">
	<option value="Three">
</datalist>
<button id="showDatalist">Show</button>
	<% @tables.each do |table| %>
		<% unless table.id == 1 %>
		<div class='tableImage col-xs-5 col-sm-4 col-md-3 col-lg-2 col-xl-1'>
			<a href="/front/table_detail/<%=table.id%>">
				<% if table.occupied %>
					<img id='image' src="/assets/table2.png" >
				<% else %>
					<img id='image' src="/assets/table1.png" >
				<% end %>
			</a>
			<p id="text">T<%=table.table_number.to_s.rjust(2,'0')%></p>
			<p id="text2">0</p>
		</div>
		<% end %>
	<% end %>
